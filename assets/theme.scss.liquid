        padding: 0 0 0 40px;
      }

      .product-feature__image--primary {
        margin: 0 auto 0 0;
      }
      .product-feature__image--secondary {
        margin-right: 0;
        margin-left: auto;
      }
    }

    &.product-feature--even {
      .product-feature__text {
        padding: 0 40px 0 0;
      }

      .product-feature__image--primary {
        margin: 0 0 0 auto;
      }
      .product-feature__image--secondary {
        margin-right: auto;
        margin-left: 0;
      }
    }

    .product-feature__image {
      width: 62%;
    }

    .product-feature__image--primary {
      position: relative;
    }

    .product-feature__image--secondary {
      margin-top: calc(100% * 0.12 * -1);
    }
  }

  @include at-query($min, $largest) {
    .product-feature__images {
      width: calc(100% - 515px);
      max-width: 50%;
    }

    .product-feature__text {
      width: 440px;
      min-width: calc(50% - 115px);
      padding: 0 !important;
    }
  }

  @include at-query($max, $medium) {
    &.product-feature--odd {
      .product-feature__image--primary {
        margin: 0 auto 0 0;
      }
      .product-feature__image--secondary {
        margin-left: auto;
        margin-right: 0;
      }
    }

    &.product-feature--even {
      .product-feature__image--primary {
        margin: 0 0 0 auto;
      }
      .product-feature__image--secondary {
        margin-right: auto;
        margin-left: 0;
      }
    }
  }
}

.product-feature--even {
  @include at-query($min, $large) {
    flex-direction: row-reverse;
  }

  .product-feature__text--overlapping {
    @include at-query ($min, $large) {
      position: relative;
      right: -12.5%;
    }
  }
}

.product-feature--odd {
  .product-feature__text--overlapping {
    @include at-query ($min, $large) {
      position: relative;
      left: -12.5%;
    }
  }
}

.product-feature--text-left {
