/*============================================================================
  #Pagination
==============================================================================*/
.pagination {
  margin-bottom: 1em;
  text-align: center;

  > span {
    display: inline-block;
    line-height: 1;
  }

  a {
    display: block;
  }

  a,
  .page.current {
    padding: 8px;
  }
}

/*============================================================================
  #Site Header
==============================================================================*/
.site-header {
  opacity: 1;
  position: absolute;
  background-color: $colorHeader;
  z-index: 800;
  .grid--table {
    display: inline-table;
    table-layout: fixed;
    width: 100%;

    > .grid__item {
      float: none;
      display: table-cell;
      vertical-align: middle;

      .site-header__logo-link {
        display: inline-block;
        vertical-align: middle;
        line-height: 1;
      }
    }
  }
}

.scrolled {
  transition: background-color 0.2s ease;
}

.header-offset {
 background-color: transparent;
}

.full-header header.site-header {
    background: transparent;
}
.site-header__logo {
  text-align: left;
  margin: 0 auto;
  max-width: 100%;

  @include at-query ($min, $large) {
    text-align: left;
  }

  a,
  a:hover,
  a:focus {
    text-decoration: none;

    animation: none;
    -webkit-mask-image: none;
    -webkit-mask-size: auto;
  }

  a, img {
    display: inline-block;
    color:$navColor;
    font-family: $font-stack-heading;
    font-weight: $font-weight-heading;
    font-style: $font-style-heading;
    text-transform: capitalize;
    @include size-header(25px);
  }

  img {
    margin: 0 auto;
    max-height: 200px;
  }
}

.site-header__logo-link {
  margin: 0 auto;
  padding-right: 10px;
  position: relative;
}

.no-js{
  body {
    display:block !important;
  }
}
.site-header__cart-toggle {
  display: inline-block;
  position:relative;
  color:$navColor!important;
  @include size-body(14px);
}
.site-header__search svg {
  g {
    stroke: $navColor;
  }

  path {
    fill: none;
  }
}

.search-bar {
  max-width: 100%;
  @include at-query ($max, $medium) {
    margin-left: auto;
    margin-right: auto;
  }
}

.site-header.fixed {position: relative !important;}
#shopify-section-header {position: relative;z-index: 999;}
/*============================================================================
  #Site Nav and Dropdowns
==============================================================================*/
.nav-bar {
  background-color: $colorTextBody;
}
.site-nav {
