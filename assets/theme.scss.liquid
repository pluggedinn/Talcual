.product-images {
  .flickity-prev-next-button {
    .arrow {
      fill: $colorTextBody;
    }
  }
}
.product-images__slider {
  margin: 0 auto 30px;
  max-width: 720px;

  .flickity-viewport {
    max-height: 100%;
  }

  .grid__image {
    margin: 0;
  }

  .flickity-prev-next-button {
    @include at-query($max, $medium) {
      &.previous {
        left: 10px;
      }

      &.next {
        right: 10px;
      }
    }

    svg {
      left: 0;
      top: 0;
      width: 100%;
      height: 100%;
    }
  }

  &:not(.flickity-enabled) {
    .product-images__slide:nth-child(n + 2) {
      display: none;
    }
  }
}
.product-images__slide {
  width: 100%;
  position: relative;

  img {
    position: absolute;
    object-fit: contain;
  }

  img.featured-image {
    position: static;
    display: block;
    margin: auto;
    width: 100%;
    height: auto;
    max-width: 100%;
    max-height: 655px;
    
    @include at-query ($max, $largish) {
      max-width: 450px;
      max-height: 450px;
    }

    @include at-query ($max, $medium) {
      max-width: 350px;
      max-height: 350px;
    }

    &[data-aspectratio*="0."] {
      height: 655px;
      width: auto;
    }

    &.lazyload,
    &.lazyloading {
      opacity: 0;
    }

    &.lazyloaded {
      opacity: 1;
    }
  }

  .mfp-content & {
    position: relative;
    padding-top: 100%;

    img {
      position: absolute;
      top: 50%;
      left: 50%;
      width: 100%;
      max-height: 100%;
      transform: translate(-50%, -50%);

      &[data-aspectratio*="0."] {
        height: 100%;
        width: auto;
      }
    }
  }
}

.product-images__thumbs {
  &:not(.flickity-enabled) {
    display: flex;
    align-items: center;
    justify-content: center;
  }
}
.product-images__thumbs-slide {
  width: 60px;
  height: 60px;
  padding: 5px;
  position: relative;

  img {
    position: absolute;
    top: 50%;
    left: 50%;
    max-width: calc(100% - 10px);
    max-height: calc(100% - 10px);
    width: auto;
    height: auto;
    transform: translate(-50%, -50%);
  }

  .loader {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: $colorBody url( "{{ 'loading.svg' | asset_url }}" ) no-repeat center center;
    background-size: 24px auto;
    opacity: 0;
    visibility: hidden;
    transition: opacity .15s ease, visibility 0s linear .15s;
  }
}

.related-products {
  padding-top: 100px;
  h4{
    padding-bottom: 50px;
  }
}


