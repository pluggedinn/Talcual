  }
}

.subcollection__item {
  display: block;
  width: auto;
  padding-right: 20px;
  position: relative;
  
  @include at-query ( $min, $large ) {
    padding: 0;
    margin-right: 20px;
  
    &:last-child {
      margin-right: 0;
    }

    .subcollections__list--3 & { width: calc(33.33% - 20px); }
