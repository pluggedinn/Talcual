.site-header__logo a {display:inline-block; color: $navColor !important;}
.site-header h1 {line-height:100%}
.site-header__logo {line-height:100%;}
.header-offset{display:none;}


/*================ Dropdowns ================*/
.site-nav__dropdown {
  display: none;
  background: $dropdownBg;
  position: absolute;
  left: 0;
  margin: 0;
  z-index: $zindexNavDropdowns;
  border-radius: 3px;

  .supports-no-touch .site-nav--has-dropdown:hover &,
  .site-nav--has-dropdown.nav-hover &,
  .nav-focus + & {
    display: block;
  }

  a {
    color: $colorTextBody;

    &:hover,
    &:active,
    &:focus {
       color: darken($colorTextBody, 10%); 
    }
  }
    li {
      position: relative;
      border-bottom: 1px solid lighten($dropdownTextColor, 80%);
    }
    li:last-of-type{
      border-bottom: none;
    }
}

.cssmenu ul ul li:last-child {
  border-bottom: none;
}

.site-nav__dropdown a {
  padding: 8px 15px;
}

.site-nav__dropdown {
  border: 1px solid $colorBorder;
  text-align:left;
}

.site-nav--compress {
  .site-nav__expanded-item {
    display: none;
  }
}

.site-nav--compress__menu {
  display: none;

  .site-nav--compress & {
    display: inline-block;
  }
}

.site-header__cart-toggle {
  margin: 0;
  vertical-align: middle;
}

.drawer-nav__has-sublist {
  display:inline-block;
  margin-right: -20px;
}

button:focus {outline:0;}

.has-sub-links:hover {
  & + .sub-sub-links{
    display: block;
  }
}
.sub-sub-links:hover{
  display: block;
}

.has-sub-links {
  margin-bottom:0px;
  a {padding-right:50px;}
}
.sub-sub-links {
  border-radius: 3px;
  display: none;
  min-width: 100%;
  position: absolute;
  top: -5px;
  left: 100%;
  background: $dropdownBg;
  border: 1px solid $dropdownLight;
  border-top-left-radius: 0px;
  border-bottom-left-radius: 0px;
  border-left: none;

  .site-nav__link {
    display: block;
    width: 100%;
    padding-left: 0px;
    border-left: 1px solid;
    border-left-color: $dropdownLight;
    padding-right: 25px;
    padding-left: 25px;
  }
  .site-nav__link:first-of-type {
    border-left: none;
  }
}

.sub-sub-links:first-of-type{
  top:-1px;
}

.megamenu__links {position: relative;}

.has-sub-links:hover {
   & > .site-nav__link + .sub-drawer-nav__toggle {
    right: 6px;
  }
}

/* effect-shine */
@-webkit-keyframes shine {
  from {
    -webkit-mask-position: 150%;
  }
  
  to {
    -webkit-mask-position: -50%;
  }
}

/*============================================================================
  #Mega menu
==============================================================================*/
.site-nav__item--has-meganav {
  position: static;
  padding-bottom: 10px;
  margin-bottom: -10px;

  &:hover {
    padding-bottom: 30px;
    margin-bottom: -30px;
  }
}
.megamenu {
  position: absolute;
  top: 100%;
  left: 0;
  width: 100%;
  z-index: -1;
  text-align: left;
  color: $dropdownTextColor;
  background: $dropdownBg;
  box-shadow: 0 1px 3px rgba(0, 0, 0, .15);
  opacity: 0;
  visibility: hidden;
  transition: opacity .2s ease, visibility 0s linear .3s;
  max-height: calc(100vh - 109px);
  overflow-x: hidden;
  overflow-y: auto;

  .nav-hover & {
    opacity: 1;
    visibility: visible;
    transition: opacity .2s ease, visibility 0s linear 0s;    
  }

  .megamenu__links {
    float: left;
    width: calc(25% - 50px);
    margin: 0 25px;
    padding: 0 50px 50px;

    &::before {
      content: '';
      position: absolute;
      top: 0;
      left: -25px;
      width: 2px;
      height: 100%;
      background: $colorBorder;
      opacity: .25;
    }

    &:nth-child(4n+1) {
      clear: both;

      &::before {
        content: none;
      }
    }

    .site-nav__link {
      display: block;
      padding: 5px 0;
      opacity: .85;
      @include size-body(14px);

      span {
        display: inline-block;
      }

      &:hover > span {
        -webkit-mask-image: linear-gradient(-75deg, rgba($dropdownTextColor, .6) 0%, $dropdownTextColor 50%, rgba($dropdownTextColor, .6) 100%);
        -webkit-mask-size: 200%;
        animation: shine 1.5s infinite;
      }
    }

    & > li > .site-nav__link {
      display: block;
      margin-top: 20px;
      margin-bottom: 16px;
      padding-bottom: 8px;
      opacity: 1;
      border-bottom: 1px solid $colorBorder;
      @include size-body(18px);
    }

    .sub-drawer-nav__toggle {
      display: none;
    }
  }

  .sub-sub-links {
    display: block;
    position: static;
    border-radius: 0;
    background: transparent;
    border: 0;
    
    .site-nav__link {
      padding: 0;
      border: 0;
      margin-bottom: 6px;
    }
  }
}

.megamenu__push + .megamenu__links::before,
.megamenu__images + .megamenu__links::before {
  content: none;
}

.megamenu-inner {
  padding: 30px 0;
  margin: 0 -25px;

  &::after {
    content: '';
    display: table;
    clear: both;
    line-height: 0;
  }
}

.megamenu--no-images {
  &.megamenu--links-1,
  &.megamenu--links-2,
  &.megamenu--links-3 {
    .megamenu-inner {
      display: flex;
      justify-content: center;
    }
  }
}

.megamenu__images {
  display: flex;
  width: 25%;

  .megamenu--left & {
    float: left;
  }

  .megamenu--right & { 
    float: right;
  }

  .megamenu--stacked & {
    float: left;
    flex-direction: column;
    width: 25%;

    .megamenu__image {
      width: calc(100% - 50px);
    }
  }

  .site-nav--no-links & {
    margin: 0 auto;
    float: none;
  }
}
.megamenu__images--2 {
  width: 50%;

  .megamenu__image {
    width: calc(50% - 50px);
  }
}
.megamenu__images--3 {
  width: 75%;

  .megamenu__image {
    width: calc(33.33% - 50px);
  }
}
.megamenu__images--4 {
  width: 100%;

  .megamenu__image {
    width: calc(25% - 50px);
  }
}

.megamenu__image {
  width: calc(100% - 50px);
  margin: 0 25px 25px;
  text-align: center;
  position: relative;

  img {
    display: block;
    width: 100%;
    margin-bottom: 10px;
  }
}

.megamenu__image-title {
  position: relative;
  display: block;
  text-transform: uppercase;
  letter-spacing: 0.5px;
  line-height: 0;

  & > span {
    display: inline-block;
    padding: 0 0 3px;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    border-bottom: 1px solid $dropdownTextColor;
    @include size-body(14px);
    line-height: normal;
  }
  
  &::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 50%;
    height: 1px;
    width: 75%;
    background: $dropdownTextColor;
    @include transform( translateX(-50%) );
    @include transition( width, .3s, ease-out );
  }

  &:hover::after {
    width: 25%;
    @include transition( width, .3s, ease-in );
  }
}

/*============================================================================
  #Mobile Nav
  - List of items inside the mobile drawer
==============================================================================*/
.mobile-nav {
  // Negative of .drawer left/right padding for full-width link tap area
  margin: -9px 0 0;
}

.mobile-nav__search {
  padding: $gutter / 2;

  .search-bar {
    margin-bottom: 0;
  }
}

.mobile-nav__item {
  position: relative;
  display: block;
  border-bottom: 1px solid $colorBorder;
  padding-bottom: 5px;
  margin-bottom: 7px;

  // Background color on top level items so there is no
  // element overlap on subnav's CSS toggle animation
  .mobile-nav > & {
    background-color: $colorBody;
  }

  .mobile-nav > &:last-child:after {
    display: none;
  }
}

// Login/logout links can't have a class on them, so style <a> element
.mobile-nav__item a {
  display: block;
}

.mobile-nav__item a,
.mobile-nav__toggle button {
  color: $colorTextHeading;
  text-decoration: none;

  &:active,
  &:focus {
   background-color: setActiveDrawers($colorBody);
  }

  .icon.icon-minus {
    display: none;
  }

  &.is-expanded {
    & > .icon {
      &.icon-plus {
        display: none;
      }

      &.icon-minus {
        display: block;
      }
    }
  }
}

.mobile-nav__item--active {
  font-weight: inherit;
}

.mobile-nav__has-sublist {
  display: table;
  width: 100%;

  .mobile-nav__link {
    display: table-cell;
    vertical-align: middle;
    width: 100%;
    h5 {margin-bottom:0px;}
  }
}

.mobile-nav__link h3 {
  line-height:1;
  margin: 2px 0;
  font-family: $font-stack-body;
  font-weight: $font-weight-body;
  font-style: $font-style-body;
}

.mobile-nav__link {
  padding: 5px 0;
}

.mobile-nav__toggle {
  display: table-cell;
  vertical-align: middle;
  width: 1%;
  padding-left: 50px;
  color: $colorTextHeading;
  margin-bottom: 0px;
}

.mobile-nav__toggle-open {
  .mobile-nav--expanded & {
    display: none;

    &.is-expanded {
      display: block;
    }
  }
}

.mobile-nav__toggle-close {
  display: none;

  .mobile-nav--expanded & {
    display: block;
  }
}

.mobile-nav__sublist {
  h5{
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
  }
  margin: 0;
  display: none;
  overflow: hidden;
  margin-left: 45px;
  padding: 10px 0;
  @include backface();

  .mobile-nav--expanded + & {
    visibility: visible;
    max-height: 700px;
    transition: all 700ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  }
  
  .mobile-nav__item {
    border: 0;
    margin-bottom: 0;
    padding-bottom: 0;
  }

  .mobile-nav__item:after {
    top: 0;
    bottom: auto;
  }

  &:last-child {
    padding-bottom: 0px;
  }

  margin-top: -3px;
}


.mobile-nav__subsublist {
  display: none;
  border-left: 1px solid rgba($colorBorder, .5);
  padding-left: 17px;
  margin: 5px 0 5px 32px;
  
  h5 {
    font-size: $baseFontSize*.9;
  }

  .mobile-nav__link {
    padding: 7px 0;
  }
}

.has-sublist a {
  h5 {float:left !important;}
}

.show-dropdown {
  visibility: visible;
  max-height: inherit;
}

.js-drawer-close {
  .custom-icon-x{
    color: $colorTextHeading;
  }
}


/*============================================================================
  #Drawers
==============================================================================*/

.drawer {
  @include promote-layer();
  display: none;
  position: fixed;
  overflow-y: auto;
  overflow-x: hidden;
  top: 0;
  bottom: 0;
  padding: 0 ($gutter / 2) ($gutter / 2);
  max-width: 95%;
  z-index: $zindexDrawer;
  color: $colorDrawerText;
  background-color: $colorBody;
  transition: $drawerTransition;

  a, h3 {
    color: $colorTextHeading;
    &:hover,
    &:focus,
    &:active {
      transition: all 0.2s ease;
      color: darken($colorTextHeading,20%);
    }
  }

  input {
    border: none;
  }
  textarea {
    border-color: $colorBorder;
  }
}

.drawer--left {
  width: $drawerNavWidth;
  right: -$drawerNavWidth;
  border-right: 1px solid $colorDrawerBorder;
  border-left: 2px solid $colorAccent;

  .js-drawer-open-left & {
    display: block;
    @include transform(translateX(-$drawerNavWidth));

    .lt-ie9 & {
      left: 0;
    }
  }
}

.drawer--right {
  width: $drawerCartWidth;
  right: -$drawerCartWidth;
  
  border-left: 2px solid $colorAccent;

  .js-drawer-open-right & {
    display: block;
    @include transform(translateX(-$drawerCartWidth));

    .lt-ie9 & {
      right: 0;
    }
  }
}

#PageContainer {
  overflow: hidden;
  background: inherit;
}
.full-header #PageContainer {
    margin-top: -90px;
}
.is-moved-by-drawer {
  @include promote-layer();
  transition: $drawerTransition;
  will-change: initial;
  .js-drawer-open-left & {
    @include transform(translateX(-$drawerNavWidth));
  }

  .js-drawer-open-right & {
    @include transform(translateX(-$drawerCartWidth));
  }
}

.drawer__header {
  display: table;
  height: 70px;
  width: 100%;
}

.drawer__title,
.drawer__close {
  display: table-cell;
  vertical-align: middle;
}

.drawer__title {
  width: 100%;
}

.drawer__close {
  width: 1%;
  text-align: center;
  font-size: em(18px);
}

.drawer__close button {
  position: relative;
  right: -20px;
  height: 100%;
  padding: 0 20px;
  color: inherit;

  &:active,
  &:focus {
    .custom-icon-x {
      color: lighten($colorTextHeading, 20%) !important;
    }
  }
  .custom-icon-x{
    font-size:20px;
    vertical-align: bottom;
  }
}

.cart-drawer-spacer-line{
  border-bottom: 2px solid $colorAccent;
  clear: both;
  display: block;
  width: 100%;
  margin-bottom: $gutter/2;
}
.drawer-button{margin-top:20px;}

#NavDrawer button {
  float: right;
  z-index: 1000;
}

/*============================================================================
  #Nav search
==============================================================================*/
.nav-search {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  min-height: 280px;
  padding-top: 73px;
  box-sizing: content-box;
  opacity: 0;
  z-index: 700;
  background-color: $colorBody;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
  visibility: hidden;
  transition: $drawerTransition, opacity .35s ease, visibility 0s linear .35s;
  will-change: initial;
  
  .search-form {
    position: relative;
    width: 500px;
    max-width: 100%;
    margin: 100px auto 30px;
    @include transition(unquote('margin-top .25s ease-in'));
  }

  @include at-query($max, $preLargest) {
    display: none !important;
    visibility: hidden;
  }

  .nav-search__submit {
    display: none;
  }

  .js-drawer-open-left & {
    @include transform(translateX(-$drawerNavWidth));
  }

  .js-drawer-open-right & {
    @include transform(translateX(-$drawerCartWidth));
  }
}
.nav-search--is-visible {
  opacity: 1;
  visibility: visible;
  transition: $drawerTransition, opacity .6s ease-out, visibility 0s linear 0s;
}
.nav-search__container {
  position: relative;
  max-width: 85%;
  width: 1186px;
  margin: 0 auto;

  &.is-searching {
    .results::before {
      content: '';
      display: block;
      width: 32px;
      height: 32px;
      background: url({{ 'loading.svg' | asset_url }}) no-repeat center center;
      background-size: contain;
      margin: 39px auto 39px;
    }

    .results__popular-links + .results::before {
      margin-top: -27px;
      margin-bottom: 31px;
    }
  }

  &.push-up {
    .search-form {
      margin-top: 20px;
    }
  }
}
.nav-search__scroller {
  max-height: 60vh;
  overflow-x: hidden;
  overflow-y: auto;
}
.nav-search__input-group {
  position: relative;
  height: 40px;
  margin: 120px auto 0;
  width: 90%;

  @include at-query($min, $large) {
    margin: 0 auto;
    width: 100%;
    height: 50px;
  }
}
.nav-search__input {
  height: 100%;
  line-height: normal;
  padding: 0 30px 0 0;
  width: 100%;
  border: 0;
  border-radius: 0;
  -webkit-appearance: none;
  box-sizing: border-box;
  color: $navColor;

  @include at-query($min, $largest) {
    padding: 0 40px;
    height: 100%;
    box-sizing: border-box;
    box-shadow: none;
    border-bottom: 2px solid $navColor;
    outline: none;
    text-align: center;
    font-size: 24px;
    font-weight: 300;
  }

  &::-webkit-input-placeholder { color: $navColor; }
  &::-moz-placeholder { color: $navColor; }
  &:-ms-input-placeholder { color: $navColor; }
  &:-moz-placeholder { color: $navColor; }
}
.nav-search__close {
  position: absolute;
  top: 0;
  right: -10px;
  height: 100%;
  display: flex;
  align-items: center;

  path {
    fill: $navColor;
  }
}
.nav-search__submit {
  display: block;
  position: absolute;
  right: 0px;
  top: 0;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 40px;
  padding: 0;
  font-size: 1.25rem;

  @include at-query($min, $large) {
    width: 60px;
  }
}
.nav-search__label-icon {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  display: flex;
  align-items: center;

  svg {
    width: 25px;
    height: 25px;
    stroke: $navColor;

    g {
      stroke: inherit;
    }
  }
}

/*============================================================================
  #Nav search results
==============================================================================*/
.results {

  .pagination {
    margin: 20px 0 40px;

    .loadMore {
      margin: 0;
    }
  }

  .no-results {
    margin: 44px 0;
    text-align: center;
  }

  .loadMore--endlessScroll {
    visibility: hidden;
    opacity: 0;
  }

  a {
    color: $dropdownTextColor;
  }
}

.results__popular-title {
  @include size-header(18px);
  text-align: center;
  margin: 0 0 13px;
}

.results__popular-links {
  @include size-header(22px);
  text-align: center;
  margin-bottom: 45px;

  li {
    display: inline-block;
    margin: 0 20px 20px;
  }

  a {
    // color: $c-text;
  }

  & + .results {
    .results__count,
    .no-results {
      margin-top: -30px;
    }
  }
}

.results__count {
  // @include h2;
  text-align: center;
  margin: 0 0 30px;
}

.results__list {
  list-style: none;
  margin: 20px -10px 0;
  display: flex;
  flex-wrap: wrap;

  .result {
    width: calc(25% - 20px);
    margin: 0 10px 40px;
    text-align: center;
  }

  .result__image {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 200px;
    margin-bottom: 15px;

    a {
      display: flex;
      align-items: center;
      justify-content: center;
      width: 100%;
      height: 100%;
      position: relative;
      cursor: default;
    }

    img {
      display: block;
      max-width: 100%;
      max-height: 100%;
      height: auto;
      cursor: pointer;
    }

    .product-status-flag {
      display: flex;
      align-items: center;
      justify-content: center;
    }
  }

  .result__title {
    margin: 0;
  }
}

/*============================================================================
  #Product and Collection Grids
==============================================================================*/
.grid__image {
  position: relative;
  display: block;
  margin: 0 auto ($gutter / 2);
  img {
    display: block;
    margin: 0 auto;
  }
  &:hover {
    cursor: pointer;
  }
  .lazyload {display: none;}
}

.supports-js .grid__image .lazyload {display: block;}

/*============================================================================
  #Collection Filters
==============================================================================*/
.filter--active {
  font-weight: $font-weight-bold;
}

/*============================================================================
  #Breadcrumbs
==============================================================================*/
.breadcrumb {
  margin-bottom: $gutter;

  a,
  span {
    display: inline-block;
    padding: 0 7px 0 0;
    margin-right: 7px;

    &:first-child {
      padding-left: 0;
    }
  }
}

/*============================================================================
  #Notes and Form Feedback
==============================================================================*/
.note,
.errors {
  border-radius: $radius;
  padding: 6px 12px;
  margin-bottom: $gutter / 2;
  border: 1px solid transparent;
  @include size-body(12px);
  text-align: center;

  ul,
  ol {
    margin-top: 0;
    margin-bottom: 0;
    list-style: none outside;
  }

  li:last-child {
    margin-bottom: 0;
  }

  p {
    margin-bottom: 0;
  }
}

.errors {
  background-color:white;
  ul {
    list-style: none outside;
    margin-left: 20px;
  }
}

.form-success {
  color: $successGreen;
  border-color: $successGreen;

  a {
    color: $successGreen;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}

.form-error,
.errors {
  color: $errorRed;
  border-color: $errorRed;

  a {
    color: $errorRed;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}


/*============================================================================
  #Ajax Cart Styles (conditionally loaded)
==============================================================================*/
.ajaxcart__inner {
  margin-bottom: $gutter;
}

.ajaxcart__row {
  > .grid {
    margin-left: -$gutter / 2;

    > .grid__item {
      padding-left: $gutter / 2;
    }
  }
}

.ajaxcart__product {
  position: relative;
  max-height: 500px;

  &.is-removed {
    max-height: 0;
    overflow: hidden;
    visibility: hidden;
    transition: all 450ms cubic-bezier(0.57,.06,.05,.95);
    @include backface();
  }
}

.ajaxcart__row {
  padding-bottom: $gutter / 2;
  margin-bottom: $gutter / 2;
  p {
    margin-bottom: 0px;
  }
}

.ajaxcart__product-image {
  display: block;
  overflow: hidden;
  margin-bottom: 15px;
  margin-top:20px;

  img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
  }
}

.ajaxcart__product-name {
  display: block;
  color: $colorTextHeading !important;
