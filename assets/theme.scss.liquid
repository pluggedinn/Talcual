    @include transform( rotate(90deg) );
  }

  .icon {
    width: 24px;
    height: 24px;
  }

  .icon--search,
  .icon--user {
    g {
      stroke: $navColor;
    }
  }

  .icon--cart {
    height: 27px;
    margin-bottom: 2px;
    path {
      fill: $navColor;
    }
