/* Collection filters */
.collection-section {
  position: relative;
}

.collection__filters-wrapper {
  position: absolute;
  width: 100%;
  padding: 20px 0;
  z-index: 100;
  background: $colorBody;

  &.is-fixed {
    top: 0;
    position: fixed;
  }

  &.is-bottom {
    position: absolute;
    top: auto;
    bottom: 0;
  }
}

.collection__filter-tag--default {
  margin-bottom: 10px;
  padding-right: 15px;

  button {
    display: block;
    padding-left: 25px;

    &::before {
      content: '';
      float: left;
      margin: 3px 0 0 -25px;
      width: 15px;
      height: 15px;
      border: 1px solid $colorBorder;
      background: #fff;
    }
  }

  &.is-active {
    button {
      &::before {
        content: '';
        color: #fff;
        border-color: #333;
        background: #333 url("{{ 'icon-check.svg' | asset_url }}") no-repeat center center;
        background-size: 10px auto;
      }
    }
  }
}

.collection__filter-tag--swatch.swatch {

  &.is-active {
    .swatch-element {
      label {
        &::after {
          opacity: 1;
          visibility: visible;
        }
      }
    }
  }

  .swatch-element {
    width: 60px;
    flex-direction: column;
    align-items: center;
    margin: 0 2px 10px;
    line-height: 1.2;

    label {
      float: none;
      display: block;
      width: {{ swatch_width }};
      position: relative;
      padding: 0;
      margin: 0 auto 5px;
    }

    &::after {
      position: static;
      margin-top: 4px;
      background-color: transparent;
      padding: 0;
      border-radius: 0;
      box-shadow: none;
      border: 0;
      opacity: 1;
      visibility: visible;
      font-size: 11px;
      line-height: 1.2;
      white-space: normal;
      @include transform( 'none' );
    }
  }
}

.collection__filter-tag--number {
  display: inline-block;
  padding: 0;
  margin: 0 6px 10px 0;

  button {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 6px 3px;
    height: 31px;
    min-width: 31px;
    border-radius: 16px;
    line-height: 17px;
    border: 1px solid $colorBorder;
    margin: 0;
  }

  &.is-active {
    button {
      color: #fff;
      border-color: #333;
      background: #333;
    }
  }
}

/* Collection filters top & mobile layout */
.collection__filters {
  height: 45px;

  h3 {
    padding-top: 5px;
    padding-bottom: 5px;
    margin-bottom: 0;
  }

  .collection__filters-wrapper {
    padding: 0;
    background: $colorBody;
    border-bottom: 1px solid $colorBorder;
  }

  .collection__filter {
    position: static;
    margin-right: 50px;
    @include transition( opacity, .3s, ease );

    @include at-query ( $max, $small ) {
      margin-right: 30px;
    }

    &:last-child {
      margin-right: 0;
    }

    &.is-expanded {
      .collection__filter-tag {
        animation: .3s ease-in-out both fade-in;

        @for $i from 1 through 100 {
          &:nth-child(#{$i}) {
             animation-delay: $i * (.03s);
          }
        }
      }
      .collection__filter-tags {
        opacity: 1;
        visibility: visible;
        margin-top: 1px;
        transition: opacity .3s ease, margin .3s ease, visibility 0s linear 0s;
      }

      .collection__filter-title {
        &::before {
          transform: rotate(270deg);
        }
      }
    }
  }

  &.has-tags-expanded {
    .collection__filter:not(.is-expanded) {
      opacity: .5;
    }
  }

  .collection__filter-title {
    padding-right: 25px;
    position: relative;
    cursor: pointer;

    &::before {
      content: "\e900";
      font-weight: bolder !important;
      font-family: 'custom-icons';
      position: absolute;
      top: 0;
      right: 0;
      transform: rotate(90deg);
      font-size: 80%;
      line-height: 1;
      margin: 5px 5px 5px 15px;
      transition: opacity .3s ease;
    }
  }

  .collection__filters-list {
    padding: 10px 0;
  }

  .collection__filter-tags {
    position: absolute;
    top: 100%;
    right: 0;
    left: 0;
    margin: 11px 0 0;
    width: 100%;
    list-style: none;
    opacity: 0;
    visibility: hidden;
    transition: opacity .3s ease, margin .3s ease, visibility 0s linear .3s;
    background: $colorBody;
    padding: 10px 50px 0;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    border-bottom: 1px solid $colorBorder;

    @include at-query ( $max, $medium ) {
      display: block;
      width: auto;
      padding: 15px 20px 10px;
      overflow: auto;
      max-height: calc(100vh - 42px);
      text-align: center;
    }
  }

  .collection__filter-tag--swatch.swatch {
    margin: 0;

    .swatch-element {
      span {
        display: block;
        border-radius: 50%;
      }
    }

    @include at-query ( $max, $medium ) {
      display: inline-block;
      margin: 0;
    }
  }

  @include at-query ( $min, $large ) {
    .collection__filter-tag--number + .collection__filter-tag--default {
      margin-left: 20px;
    }

    .collection__filter-tag--number ~ .collection__filter-tag--default {
      margin-top: 5px;
    }
  }
}

.collection__filter {
  position: relative;

  h3 {
    position: relative;
    padding-right: 30px;

    .custom-icon-arrow-right {
      position: absolute;
      top: 0;
      right: 0;
      font-size: 80%;
      line-height: 1;
      margin: 8px 0 0;
      @include transform( rotate(90deg) );
    }
  }

  button {
    @include size-body(14px);
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
    color: inherit;
  }
}

.collection__products {
  position: relative;

  .selector-wrapper {
    padding-right: 0;

    &::after {
      right: 10px;
    }
  }

  .sort-me {
    z-index: 2;
  }

  &::before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    z-index: 100;
    opacity: 0;
    visibility: hidden;
    display: block;
    width: 100%;
    height: 100%;
    background: #fff url( "{{ 'loading.svg' | asset_url }}" ) no-repeat center 25vh;
    background-size: 100px 100px;
    transition: opacity .3s ease, visibility 0s linear .3s;
  }

  .is-loading & {
    height: 100vh;
    overflow: hidden;

    &::before {
      opacity: 1;
      visibility: visible;
      transition: opacity .3s ease, visibility 0s linear 0s;
    }
  }

  .no-matches {
    @include size-body(16px);
  }
}

.reset-filters {
  display: inline-block;
  margin: 0;
  font-family: $font-stack-body;
  font-weight: $font-weight-body;
  font-style: $font-style-body;
  color: inherit;
  text-align: left;
  @include size-body(14px);

  .icon {
    float: left;
    margin: -2px 2px -4px -5px;
    width: 26px;
    height: 26px;
    @include transition( transform, .3s, ease );
  }

  &:hover {
    .icon {
      transform: rotate(90deg);
    }
  }
}

.collection__filters-list {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  
  .reset-filters {
    margin-right: 50px;
  }

  @include at-query ( $max, $small ) {
    justify-content: center;

    .reset-filters {
      margin-right: 30px;
    }
  }
}

.collection__filters--left {
  @include at-query ( $min, $largest ) {
    height: auto;
    .collection__filters-wrapper {
      border: 0;
      background-color: transparent;
      padding-top: 10px;
      left: 0;
      width: 350px;
      max-height: 100vh;

      &.is-fixed {
        overflow-x: hidden;
        overflow-y: auto;
      }
    }

    &.has-tags-expanded .collection__filter:not(.is-expanded) { opacity: 1; }

    .collection__filters-list {
      padding: 10px 0;
      justify-content: flex-start;
      flex-direction: column;
      width: 200px;

      .reset-filters {
        margin: 0 0 15px;
      }
    }

    & ~ .collection__products {
      padding-left: 350px;
      min-height: 100vh;

      .is-loading & {
        height: auto;
      }

      .collection--no-results {
        padding-right: 300px;
      }
    }

    .collection__filter {
      margin-bottom: 40px;
      margin-right: 0;

      &:last-child {
        margin-bottom: 0;
      }
    }

    .collection__filter-title {
      display: block;
      border-bottom: 1px solid $colorBorder;
      padding-bottom: 5px;
      padding-right: 0;
      margin-bottom: 15px;
      cursor: default;
      text-transform: uppercase;
      letter-spacing: 0.1em;
      @include size-body(12px);

      &::before,
      &::after {
        content: none;
      }
    }

    .collection__filter-tag {
      transition: none;
      opacity: 1;
    }
    .collection__filter-tags {
      list-style: none;
      margin: 0;
      padding: 0;
      position: static;
      opacity: 1;
      visibility: visible;
      background-color: transparent;
      display: block;
      border-bottom: 0;

      &::before {
        content: none;
      }
    }

    .collection__filter-tag--swatch.swatch {
      margin: 0 7px 9px 0;
      display: inline-block;

      .swatch-element {
        margin: 0;
        display: block;
        width: auto;

        &::after {
          position: absolute;
          top: 100%;
          background: $colorBody;
          color: $colorPrimary;
          padding: 2px 5px;
          border-radius: 3px;
          box-shadow: 0 1px 3px rgba(0, 0, 0, .15);
          border: 1px solid $colorBorder;
          white-space: nowrap;
          opacity: 0;
          visibility: hidden;
          transition: none;
          @include size-body(11px);
          @include transform(translateY(50%));

          .supports-touch & {
            content: none;  
          }
        }

        &:hover {
          z-index: 5;
          &::after {
            opacity: 1;
            visibility: visible;
            transition: transform .3s ease, opacity .3s ease, visibility 0s linear 0s;
            @include transform(translateY(0));
          }
        }
      }
    }

    .collection__filter-tag--number + .collection__filter-tag--default {
      margin-left: 0;
      margin-top: 5px;
    }
  }
}

// Filter tags animation
@keyframes fade-in {
  0% {
    opacity: 0;
    transform: scale(0);
  }
  
  100% {
    opacity: 1;
    transform: scale(1);
  }
}

/* overlay at start */
.mfp-fade.mfp-bg {
  opacity: 0;

  -webkit-transition: all 0.15s ease-out;
  -moz-transition: all 0.15s ease-out;
  transition: all 0.15s ease-out;
}
/* overlay animate in */
.mfp-fade.mfp-bg.mfp-ready {
  opacity: 0.8;
}
/* overlay animate out */
.mfp-fade.mfp-bg.mfp-removing {
  opacity: 0;
}

/* content at start */
.mfp-fade.mfp-wrap .mfp-content {
  opacity: 0;

  -webkit-transition: all 0.15s ease-out;
  -moz-transition: all 0.15s ease-out;
  transition: all 0.15s ease-out;
}
/* content animate it */
.mfp-fade.mfp-wrap.mfp-ready .mfp-content {
  opacity: 1;
}
/* content animate out */
.mfp-fade.mfp-wrap.mfp-removing .mfp-content {
  opacity: 0;
}
