  padding-right: 40px;

  @include at-query ( $min, $large ) {
    [data-columns="1"] & { width: calc( (100vw - 100px) * 0.9 ); }
    [data-columns="2"] & { width: calc( (100vw - 100px) * 0.45 ); }
    [data-columns="3"] & { width: calc( (100vw - 100px) * 0.3 ); }
    [data-columns="4"] & { width: calc( (100vw - 100px) * 0.23 ); }
  }

  @include at-query ( $max, $preLargest ) {
    padding-right: 25px;
  }

  @include at-query ( $max, $medium ) {
    width: calc( (100vw - 50px) * 0.8 );
  }
}

.list-collections__slide-image {
  display: block;
  position: relative;
  overflow: hidden;
  height: 500px;

  &::before,
  &::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, .25);
    transition: background .5s ease;
  }

  &::after {
    left: -100%;
    right: auto;
    width: 100%;
    background: linear-gradient(to right, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, .25) 50%, rgba(255, 255, 255, 0) 100%);
  }

  &:hover {
    &::before {
      background: rgba(0, 0, 0, .15);
    }

    &::after {
      animation: move 1s ease;
    }

    h3 span::after {
      width: 100%;
      transition: width .5s ease-out;
    }
  }

  h3 {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    padding: 25px;
    margin: 0;
    color: #fff;

    span {
      display: inline-block;
      position: relative;

      &::after {
        content: '';
        position: absolute;
        top: 100%;
        left: 0;
        width: 0;
        height: 1px;
        background: #fff;
        transition: width .25s ease-in;
      } 
    }

    [data-text-position="center"] & {
      bottom: 50%;
      text-align: center;
      @include transform( translateY(50%) );
    }
  }

  [data-height="300"] & { height: 300px; }
  [data-height="400"] & { height: 400px; }
  [data-height="500"] & { height: 500px; }
  [data-height="600"] & { height: 600px; }
  [data-height="700"] & { height: 700px; }
}

.list-collections__slide-image-bg {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: $colorBody;
  background-position: center center;
  background-size: cover;
  background-repeat: no-repeat;

  .list-collections__slide--onboarding & {
    background-color: #f5f5f5;
  }
}

